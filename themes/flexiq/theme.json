{
	"$schema": "https://schemas.wp.org/wp/6.9/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"slug": "primary",
					"color": "#0F1B2D",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#2563EB",
					"name": "Secondary"
				},
				{
					"slug": "accent",
					"color": "#06B6D4",
					"name": "Accent"
				},
				{
					"slug": "base",
					"color": "#FFFFFF",
					"name": "Base"
				},
				{
					"slug": "base-alt",
					"color": "#F1F5F9",
					"name": "Base Alt"
				},
				{
					"slug": "contrast",
					"color": "#0F172A",
					"name": "Contrast"
				},
				{
					"slug": "muted",
					"color": "#64748B",
					"name": "Muted"
				},
				{
					"slug": "border",
					"color": "#E2E8F0",
					"name": "Border"
				},
				{
					"slug": "success",
					"color": "#059669",
					"name": "Success"
				},
				{
					"slug": "warning",
					"color": "#D97706",
					"name": "Warning"
				},
				{
					"slug": "error",
					"color": "#DC2626",
					"name": "Error"
				}
			],
			"gradients": [
				{
					"slug": "primary-to-secondary",
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--primary) 0%, var(--wp--preset--color--secondary) 100%)",
					"name": "Primary to Secondary"
				},
				{
					"slug": "secondary-to-accent",
					"gradient": "linear-gradient(135deg, var(--wp--preset--color--secondary) 0%, var(--wp--preset--color--accent) 100%)",
					"name": "Secondary to Accent"
				}
			]
		},
		"typography": {
			"defaultFontSizes": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
					"name": "Inter",
					"slug": "heading",
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontWeight": "600",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"src": ["file:./assets/fonts/inter-v18-latin-600.woff2"]
						},
						{
							"fontFamily": "Inter",
							"fontWeight": "700",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"src": ["file:./assets/fonts/inter-v18-latin-700.woff2"]
						}
					]
				},
				{
					"fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, sans-serif",
					"name": "Body",
					"slug": "body",
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"src": ["file:./assets/fonts/inter-v18-latin-regular.woff2"]
						},
						{
							"fontFamily": "Inter",
							"fontWeight": "500",
							"fontStyle": "normal",
							"fontDisplay": "swap",
							"src": ["file:./assets/fonts/inter-v18-latin-500.woff2"]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "clamp(0.8125rem, 0.75rem + 0.25vw, 0.875rem)",
					"name": "Small",
					"fluid": false
				},
				{
					"slug": "medium",
					"size": "clamp(1rem, 0.9rem + 0.4vw, 1.125rem)",
					"name": "Medium",
					"fluid": false
				},
				{
					"slug": "large",
					"size": "clamp(1.25rem, 1rem + 1vw, 1.5rem)",
					"name": "Large",
					"fluid": false
				},
				{
					"slug": "x-large",
					"size": "clamp(1.5rem, 1rem + 2vw, 2.25rem)",
					"name": "Extra Large",
					"fluid": false
				},
				{
					"slug": "xx-large",
					"size": "clamp(2rem, 1.25rem + 3vw, 3.5rem)",
					"name": "2X Large",
					"fluid": false
				}
			]
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{ "slug": "10", "size": "0.25rem", "name": "1" },
				{ "slug": "20", "size": "0.5rem", "name": "2" },
				{ "slug": "30", "size": "1rem", "name": "3" },
				{ "slug": "40", "size": "1.5rem", "name": "4" },
				{ "slug": "50", "size": "2rem", "name": "5" },
				{ "slug": "60", "size": "3rem", "name": "6" },
				{ "slug": "70", "size": "4rem", "name": "7" },
				{ "slug": "80", "size": "6rem", "name": "8" }
			],
			"units": ["%", "px", "em", "rem", "vh", "vw"]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true,
			"radiusSizes": [
				{ "name": "Small", "slug": "small", "size": "4px" },
				{ "name": "Medium", "slug": "medium", "size": "8px" },
				{ "name": "Large", "slug": "large", "size": "16px" },
				{ "name": "Full", "slug": "full", "size": "9999px" }
			]
		},
		"shadow": {
			"defaultPresets": false,
			"presets": [
				{
					"slug": "sm",
					"name": "Small",
					"shadow": "0 1px 2px 0 rgba(0, 0, 0, 0.05)"
				},
				{
					"slug": "md",
					"name": "Medium",
					"shadow": "0 4px 6px -1px rgba(0, 0, 0, 0.1), 0 2px 4px -2px rgba(0, 0, 0, 0.1)"
				},
				{
					"slug": "lg",
					"name": "Large",
					"shadow": "0 10px 15px -3px rgba(0, 0, 0, 0.1), 0 4px 6px -4px rgba(0, 0, 0, 0.1)"
				},
				{
					"slug": "xl",
					"name": "Extra Large",
					"shadow": "0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 8px 10px -6px rgba(0, 0, 0, 0.1)"
				}
			]
		},
		"custom": {
			"lineHeight": {
				"tight": 1.2,
				"normal": 1.6,
				"loose": 1.8
			},
			"letterSpacing": {
				"tight": "-0.025em",
				"normal": "0",
				"wide": "0.05em"
			},
			"transition": {
				"default": "all 0.2s ease-in-out"
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.6"
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--30)",
			"padding": {
				"top": "0",
				"right": "var(--wp--preset--spacing--30)",
				"bottom": "0",
				"left": "var(--wp--preset--spacing--30)"
			}
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontWeight": "700",
					"lineHeight": "1.2",
					"letterSpacing": "-0.025em"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "600"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--accent)"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--accent)"
					},
					"outline": {
						"color": "var(--wp--preset--color--secondary)",
						"width": "2px",
						"style": "solid",
						"offset": "2px"
					}
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--base)"
				},
				"border": {
					"radius": "var(--wp--preset--border-radius--medium)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontWeight": "600",
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--20)",
						"right": "var(--wp--preset--spacing--40)",
						"bottom": "var(--wp--preset--spacing--20)",
						"left": "var(--wp--preset--spacing--40)"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--base)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--primary)",
						"text": "var(--wp--preset--color--base)"
					},
					"outline": {
						"color": "var(--wp--preset--color--secondary)",
						"width": "2px",
						"style": "solid",
						"offset": "2px"
					}
				}
			},
			"input": {
				"border": {
					"color": "var(--wp--preset--color--border)",
					"radius": "var(--wp--preset--border-radius--medium)",
					"width": "1px"
				},
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--20)",
						"right": "var(--wp--preset--spacing--30)",
						"bottom": "var(--wp--preset--spacing--20)",
						"left": "var(--wp--preset--spacing--30)"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--base)",
					"text": "var(--wp--preset--color--contrast)"
				}
			}
		},
		"blocks": {
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "500",
					"letterSpacing": "0.025em",
					"textTransform": "uppercase"
				}
			},
			"core/quote": {
				"border": {
					"left": {
						"color": "var(--wp--preset--color--secondary)",
						"width": "4px",
						"style": "solid"
					}
				},
				"spacing": {
					"padding": {
						"left": "var(--wp--preset--spacing--40)"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontStyle": "normal"
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--border)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--heading)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "700",
					"letterSpacing": "-0.025em"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"customTemplates": [
		{
			"name": "page-styles",
			"postTypes": ["page"],
			"title": "Style Guide"
		}
	]
}
